  sentences:
  - text:
    - "\u201C\u6709\u4E9B\u4E1C\u897F\u5728\u6D1E\u7A74\u91CC\u662F\u6C38\u6052\u7684\u201D"
    - "\u6BD4\u5982\u8FD9\u53CC\u978B\u5B50\uFF1F<br>\u770B\u8FD9\u6837\u5F0F\u4F3C\u4E4E\u662F\u51E0\u4E2A\u4E16\u7EAA\u524D\u7684\u4EA7\u7269\u4E86\u3002<br>\u4F60\u76EF\u7740\u8FD9\u53CC\u978B\u5B50\uFF0C\u7A81\u7136\u4E00\u79CD\u6050\u60E7\u611F\u6CB9\u7136\u800C\u751F\u3002<br>\u4F60\u5BB3\u6015\u5F97\u5C06\u5176\u4E22\u5728\u4E00\u65C1\u3002"
    - "\u5B83\u8FD9\u662F\u4E00\u53CC\u666E\u901A\u7684\u978B\u5B50\u800C\u5DF2\u3002"
    events:
    - op: 1
      opInfo: 0
  - text:
    - "\u56E0\u4E3A\u5B83\u4E0E\u4F60\u89C1\u8FC7\u7684\u6240\u6709\u978B\u5B50\u62E5\u6709\u540C\u6837\u7684\u529F\u80FD\uFF0C<br>\u751A\u81F3\u662F**\u5F62\u6001**\u3002"
    events:
    - op: 1
      opInfo: 0
  - text:
    - "\u4F60\u81EA\u5DF1\u90FD\u4E0D\u6E05\u695A\u7684\u95EE\u9898\u95EE\u6211\u662F\u6CA1\u6709\u610F\u4E49\u7684\u3002"
    events:
    - op: 0
      opInfo: 0
  - text:
    - "\u6050\u60E7\u662F\u7531\u6FC0\u7D20\u89E6\u53D1\u7684\u3002\u4F60\u7684\u5927\u8111\u5206\u6CCC\u4E86\u6FC0\u7D20\u3002"
    events:
    - op: 1
      opInfo: 0
  - text:
    - "\u7A7F\u4E0D\u7A7F\u978B\u5B50\u662F\u4E00\u79CD\u81EA\u7531\u3002<br>\u53EF\u60DC\u4F60\u5728\u6D1E\u7A74\u91CC\uFF0C\u54EA\u6765\u81EA\u7531\u4E00\u8BF4\u5462\uFF1F"
    events:
    - op: 3
      opInfo: 0
  menus:
  - options:
    - text: "\u54EA\u91CC\u666E\u901A\uFF1F"
      nextSentence: 1
    - text: "\u6211\u4E3A\u4EC0\u4E48\u4F1A\u6050\u60E7\uFF1F"
      nextSentence: 2
    - text: "\u6CA1\u95EE\u9898\u3002\u6211\u7A7F\u4E0A\u5C31\u662F\u4E86\u3002"
